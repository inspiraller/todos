{
  "name": "todos-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db": "cd postgres-db && docker-compose --env-file \"../.env\" up",
    "server": "cd node-app && npm start",
    "client": "cd react-app && npm run dev",
    "start": "concurrently \"npm run db\" \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.1"
  }
}
