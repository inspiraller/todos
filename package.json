{
  "name": "todos-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db": "cd postgres-db && docker-compose --env-file \"../.env\" up --build ",
    "server-pg": "cd node-app-pg && npm start",
    "client": "cd react-app && npm run dev",
    "start": "concurrently \"npm run db\" \"npm run server-pg\" \"npm run client\"",
    "db-export": "cd postgres-db && node export.js",
    "db-clear": "npm run db-export && cd postgres-db && docker-compose down && docker volume rm postgres-db_pgdata",
    "mongo": "cd mongodb && docker-compose --env-file \"../.env\" up",
    "dockerTestArg": "cd dockerTestArg && docker-compose --env-file \"../.env\" up"
  },
  "dependencies": {
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0"
  }
}
